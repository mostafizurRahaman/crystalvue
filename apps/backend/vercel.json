{
  "buildCommand": "cd ../.. && npm run db:generate && turbo run build --filter=backend",
  "installCommand": "cd ../.. && npm install",
  "framework": null,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs22.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
